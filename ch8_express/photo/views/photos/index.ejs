<!DOCTYPE html>
<html>
<head>
	<title><%= title %></title>
	<link rel="stylesheet" type="text/css" href="/stylesheets/styles.css" />
</head>
<body>
	<h1>Photos</h1>
	<p>Express stock photo application.</p>
	<dir id="photos">
		<% photos.forEach(function(photo) { %>
		<div class="photo">
			<h2><%=photo.name %></h2>
			<a href="/photo/<%=photo.id%>/download">
				<img src='/photos/<%=photo.path%>'/>
			</a>
		</div>
		<% }) %>
	</dir>
</body>
</html>